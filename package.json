{
  "name": "abv-agent",
  "version": "0.0.1",
  "description": "Abvos browser agent",
  "engines": {
    "node": ">=6.11.3"
  },
  "main": "index.js",
  "scripts": {
    "test": "node agent.js",
    "dist": "browserify index.js -o dist/tmp.js && uglifyjs dist/tmp.js abv.js -c -m -o dist/abv-agent.0.0.1.js",
    "dev": "browserify index.js -o dist/tmp.js && uglifyjs dist/tmp.js abv.js -b -m -o dist/abv-agent.0.0.1.js"
  },
  "dependencies": {
    "abv-ts": "tondy67/abv-ts",
    "abv-core": "tondy67/abv-core",
    "jszip": "^3.1.3",
    "ws": "^3.2.0"
  },
  "devDependencies": {
    "browserify": "^14.4.0",
    "uglify-es": "^3.0.28"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tondy67/abv-agent.git"
  },
  "keywords": [
    "abvos",
    "websocket",
    "search"
  ],
  "author": {
    "name": "Todor Angelov",
    "email": "abv@tondy.org",
    "url": "http://tondy.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tondy67/abv-agent/issues"
  },
  "homepage": "https://github.com/tondy67/abv-agent#readme"
}
